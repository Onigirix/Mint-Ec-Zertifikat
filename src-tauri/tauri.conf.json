{
  "productName": "Mint-EC",
  "version": "0.0.1",
  "identifier": "de.mint-ec.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "label": "main-window",
        "title": "Mint-EC",
        "maximized": true,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/16x16.png",
      "icons/24x24.png",
      "icons/32x32.png",
      "icons/64x64.png",
      "icons/128x128.png",
      "icons/256x256.png",
      "icons/512x512.png",
      "icons/1024x1024.png",
      "icons/icon.ico",
      "icons/icon.icns"
    ],
    "resources": ["icons/*", "resources/*", "pdf-master/*"],
    "publisher": "Finn Arved Lieker"
  }
}
